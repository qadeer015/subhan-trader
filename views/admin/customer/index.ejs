<%- include('../partials/_header') %>
<div class="row">
    <div class="col-12">
        <h1>Customers</h1>
    </div>
</div>
<div class="row">
    <div class="col-12">
        <ul class="list-group">
        <% customers.forEach(customer => { %>
            <li class="list-group-item"><%= customer.name %> <span class="ms-3"><%= customer.created_at.toLocaleString() %></span></li>
        <% }) %>
        </ul>
    </div>
</div>
<%- include('../partials/_footer') %>